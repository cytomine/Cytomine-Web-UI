{
  "name": "cytomine-front-end",
  "version": "1.0.0",
  "scripts": {
    "translations-csv2json": "i18n-csv2json-cli --from src/locales/translations.csv --to src/locales/json --format",
    "watch-translations": "onchange -i 'src/locales/translations.csv' -- npm run translations-csv2json",
    "serve": "npm run watch-translations & vue-cli-service serve",
    "build": "npm run translations-csv2json && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "axios": "^0.19.2",
    "buefy": "^0.8.10",
    "bulma": "^0.7.5",
    "bulma-slider": "^2.0.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "copy-to-clipboard": "^3.2.1",
    "cytomine-client": "^1.2.0",
    "filesize": "^6.0.1",
    "ifvisible": "^1.1.0",
    "lodash": "^4.17.15",
    "npm-font-open-sans": "^1.1.0",
    "ol": "^6.2.1",
    "ol-rotate-feature": "^2.2.1",
    "sl-vue-tree": "1.8.4",
    "v-click-outside-x": "^4.1.0",
    "v-tooltip": "^2.0.3",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-color": "^2.7.0",
    "vue-draggable-resizable": "^2.1.0",
    "vue-i18n": "^8.15.3",
    "vue-moment": "^4.1.0",
    "vue-multiselect": "^2.1.6",
    "vue-notification": "^1.3.20",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.1.5",
    "vue-shortkey": "^3.1.7",
    "vue-slider-component": "^3.1.1",
    "vuelayers": "^0.11.21",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.2",
    "eslint-plugin-vue": "^6.2.1",
    "@vue/cli-service": "^4.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.8.0",
    "i18n-csv2json-cli": "^1.0.2",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
