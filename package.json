    "babel-jest": "^27.5.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.5.0",
    "eslint-plugin-jest": "^26.8.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^27.0.0",
    "jest-html-reporter": "^4.1.0",
    "onchange": "^7.1.0",
    "sass": "^1.86.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.10"
